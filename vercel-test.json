{
  "version": 2,
  "name": "qr-event-system",
  "builds": [
    {
      "src": "api/simple.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/simple.js"
    }
  ],
  "functions": {
    "api/simple.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
