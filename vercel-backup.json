{
  "version": 2,
  "name": "qr-event-system",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/diagnostic.js": {
      "maxDuration": 10
    },
    "api/simple.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/diagnostic",
      "destination": "/api/diagnostic.js"
    },
    {
      "source": "/api/simple",
      "destination": "/api/simple.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}